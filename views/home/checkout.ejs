                      <thead>
                        <tr><th>Product</th>
                        <th>Total</th>
                      </tr></thead>
                      <tbody>
                        <tr>
                          
                   <%myCart.product.forEach(prdct=>{%>
                    <% let prdcName=JSON.parse(JSON.stringify(prdct.foundProduct.name))%>


                          <td class="text-capitalize"><%=prdcName.toString()%><strong class="mx-2">x</strong> <%=prdct.productQty%></td>
                          <td>₵<%=prdct.foundProduct.price%></td>
                        </tr>
                    <%})%>
                       
                        <tr>
                          <td class="text-black font-weight-bold"><strong>Cart Subtotal</strong></td>
                          <td class="text-black">₵<%=myCart.totalCost%></td>
                        </tr>
                        <tr>
                          <td class="text-black font-weight-bold"><strong>Order Total</strong></td>
                          <td class="text-black font-weight-bold"><strong>₵<%=myCart.totalCost%></strong></td>
                        </tr>
                      </tbody>
